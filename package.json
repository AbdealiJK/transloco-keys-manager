{
  "name": "@skazaz/transloco-keys-manager",
  "version": "1.0.0-beta.24",
  "description": "Angular Transloco keys managing cli tool and Webpack plugin",
  "main": "public_api.js",
  "scripts": {
    "test": "jest",
    "start": "tsc --watch",
    "build": "npm run clean:dist && tsc && npm run copy:dist && npm run uglify",
    "uglify": "uglifyjs-folder dist --each --extension .js --output dist",
    "clean:dist": "rm -rf dist",
    "copy:dist": "cp package.json package-lock.json README.md LICENSE dist"
  },
  "bin": {
    "transloco-keys-manager": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngneat/transloco-keys-manager.git"
  },
  "keywords": [
    "angular",
    "angular 2",
    "i18n",
    "transloco",
    "translate",
    "keys",
    "tool",
    "cli",
    "webpack"
  ],
  "author": "Shahar Kazaz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngneat/transloco-keys-manager/issues"
  },
  "homepage": "https://github.com/ngneat/transloco-keys-manager#readme",
  "dependencies": {
    "@ngneat/transloco-utils": "^1.0.2",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "cli-table": "^0.3.1",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "deep-diff": "^1.0.2",
    "find": "^0.3.0",
    "glob": "^7.1.4",
    "ora": "^3.4.0",
    "os-locale": "^4.0.0"
  },
  "devDependencies": {
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "lodash.isequal": "^4.5.0",
    "typescript": "^3.6.4",
    "uglifyjs-folder": "^1.5.1"
  }
}
