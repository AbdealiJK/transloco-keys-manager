{
  "name": "@ngneat/transloco-keys-manager",
  "version": "1.0.0-rc.1",
  "description": "Extract translate keys from projects using transloco",
  "main": "public_api.js",
  "scripts": {
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors init",
    "release": "cd dist && standard-version --infile ../CHANGELOG.md",
    "commit": "git-cz",
    "test": "PRODUCTION=true jest",
    "start": "npm run prettier && tsc --watch",
    "build": "tsc && npm run copy:dist",
    "uglify": "uglifyjs-folder dist --each --extension .js --output dist",
    "clean:dist": "rm -rf dist",
    "copy:dist": "cp package.json package-lock.json README.md LICENSE dist",
    "prettier": "prettier --write 'src/**/*.ts'"
  },
  "bin": {
    "transloco-keys-manager": "index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngneat/transloco-keys-manager.git"
  },
  "keywords": [
    "angular",
    "angular 2",
    "i18n",
    "extract",
    "transloco",
    "translate",
    "keys",
    "tool",
    "cli",
    "webpack"
  ],
  "maintainers": [
    {
      "name": "Shahar Kazaz"
    },
    {
      "name": "Netanel Basal"
    }
  ],
  "bugs": {
    "url": "https://github.com/ngneat/transloco-keys-manager/issues"
  },
  "homepage": "https://github.com/ngneat/transloco-keys-manager#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/*.{ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ngneat/transloco-utils": "^1.0.6",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "columnify": "^1.5.4",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "deep-diff": "^1.0.2",
    "glob": "^7.1.4",
    "ora": "^3.4.0",
    "os-locale": "^4.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-angular": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@types/jest": "^24.0.20",
    "all-contributors-cli": "^6.8.1",
    "fs-extra": "^8.1.0",
    "git-cz": "^3.2.1",
    "husky": "^3.0.1",
    "jest": "^24.9.0",
    "lint-staged": "9.2.0",
    "lodash.isequal": "^4.5.0",
    "prettier": "^1.18.2",
    "standard-version": "^6.0.1",
    "typescript": "^3.6.4",
    "uglifyjs-folder": "^1.5.1"
  }
}
